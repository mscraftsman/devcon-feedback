{
  "import_path": "github.com/devcon-feedback",

  "bootstrap": {
    "generate": true,
    "no_grpc_wire": true,
    "settings": [
      {
        "name": "BaseURL",
        "description": "Base URL of the webservice",
        "public": true
      },
      {
        "name": "MeetupKey",
        "description": "Meetup.com API Key"
      },
      {
        "name": "MeetupSecret",
        "description": "Meetup.com API Secret"
      },
      {
        "name": "MeetupRedirectURL",
        "description": "Meetup.com Redirect URL",
        "public": true
      },
      {
        "name": "JWTSecret",
        "description": "JWT Secret Signing Token"
      },
      {
        "name": "Now",
        "description": "If set, this will be used for session voting validation"
      }
    ],
    "assets": true
  },
  "crud": {
    "generate": true
  },
  "admin": {
    "generate": true,
    "create": true,
    "read": true,
    "list": true,
    "update": true,
    "delete": true,
    "hooks": {
      "pre_create": true,
      "post_create": true,
      "pre_read": true,
      "post_read": true,
      "pre_list": true,
      "post_list": true,
      "pre_update": true,
      "post_update": true,
      "pre_delete": true,
      "post_delete": true
    },
    "auth": {
      "generate": true
    }
  },
  "vuetify": {
    "generate": true,
    "app": "admin"
  },
  "entities": [
    {
      "name": "Feedback",
      "primary_key": "uuid",
      "description": "A Feedback",
      "vuetify": {
        "icon": "work",
        "not_in_menu": false
      },
      "created_at": true,
      "default_sort": {
        "field": "created_at",
        "ascending": true
      },
      "fields": [
        {
          "label": "Reaction 1",
          "edit_widget": {
            "hide": false,
            "type": "textfield"
          },
          "list_widget": {
            "hide": false
          },
          "name": "Reaction1",
          "type": "string"
        },
        {
          "label": "Reaction 2",
          "edit_widget": {
            "hide": false,
            "type": "textfield"
          },
          "list_widget": {
            "hide": false
          },
          "name": "Reaction2",
          "type": "string"
        },
        {
          "label": "Reaction 3",
          "edit_widget": {
            "hide": false,
            "type": "textfield"
          },
          "list_widget": {
            "hide": false
          },
          "name": "Reaction3",
          "type": "string"
        },
        {
          "label": "Reaction 4",
          "edit_widget": {
            "hide": false,
            "type": "textfield"
          },
          "list_widget": {
            "hide": false
          },
          "name": "Reaction4",
          "type": "string"
        }
      ],
      "relationships": [
        {
          "entity": "Session",
          "type": "one-many"
        },
        {
          "entity": "Visitor",
          "type": "one-many"
        }
      ]
    },
    {
      "name": "Visitor",
      "primary_key": "uuid",
      "description": "A visitor",
      "vuetify": {
        "icon": "work",
        "not_in_menu": false
      },
      "created_at": true,
      "label_field": "name",
      "default_sort": {
        "field": "created_at",
        "ascending": true
      },
      "fields": [
        {
          "label": "Name",
          "edit_widget": {
            "hide": false,
            "type": "textfield"
          },
          "list_widget": {
            "hide": false
          },
          "name": "Name",
          "type": "string"
        },
        {
          "label": "Photo Link",
          "edit_widget": {
            "hide": false,
            "type": "textfield"
          },
          "list_widget": {
            "hide": false
          },
          "name": "PhotoLink",
          "type": "string"
        }
      ],
      "relationships": [
        {
          "entity": "Feedback",
          "type": "many-one"
        }
      ]
    },
    {
      "name": "Rating",
      "primary_key": "uuid",
      "description": "A rating",
      "vuetify": {
        "icon": "work",
        "not_in_menu": false
      },
      "created_at": true,
      "updated_at": true,
      "default_sort": {
        "field": "updated_at",
        "ascending": true
      },
      "fields": [
        {
          "label": "Rating Count",
          "edit_widget": {
            "hide": false,
            "type": "textfield"
          },
          "list_widget": {
            "hide": false
          },
          "name": "RatingCount",
          "type": "string"
        },
        {
          "label": "Score",
          "edit_widget": {
            "hide": false,
            "type": "textfield"
          },
          "list_widget": {
            "hide": false
          },
          "name": "Score",
          "type": "int64"
        },
        {
          "label": "Reaction Summary",
          "edit_widget": {
            "hide": false,
            "type": "textarea"
          },
          "list_widget": {
            "hide": false
          },
          "name": "ReactionSummary",
          "type": "string"
        }
      ],
      "relationships": [
        {
          "entity": "Feedback",
          "type": "many-one"
        }
      ]
    }
  ]
}